{
  "name": "centro-js",
  "description": "Pub-sub and work queue service on Primus or TCP. Supports wildcards, streams and back-pressure. Just Node and a filesystem required.",
  "version": "2.2.1",
  "homepage": "https://github.com/davedoesdev/centro",
  "author": {
    "name": "David Halls",
    "email": "dave@davedoesdev.com",
    "url": "http://www.davedoesdev.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davedoesdev/centro.git"
  },
  "bugs": {
    "url": "https://github.com/davedoesdev/centro/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "node --napi-modules ./node_modules/.bin/grunt lint keys test",
    "test-win": "node --napi-modules .\\node_modules\\grunt\\bin\\grunt lint keys test",
    "coverage": "node --napi-modules ./node_modules/.bin/grunt lint keys coverage"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "keywords": [
    "pub-sub",
    "queue",
    "primus",
    "stream",
    "back-pressure",
    "routing",
    "mqlobber",
    "qlobber-fsq"
  ],
  "dependencies": {
    "mqlobber": "~6.1.0",
    "mqlobber-access-control": "~5.2.0",
    "qlobber-fsq": "~10.0.4",
    "authorize-jwt": "~4.0.1",
    "primus": "~7.1.0",
    "primus-backpressure": "~1.0.3",
    "frame-stream": "~1.1.1",
    "ajv": "~5.5.0",
    "async": "~2.6.0",
    "access-control": "~1.0.0",
    "stream-throttle": "~0.1.3",
    "wu": "~2.1.0"
  },
  "devDependencies": {
    "grunt": "~1.0.1",
    "grunt-contrib-jshint": "~1.1.0",
    "grunt-mocha-test": "~0.13.3",
    "grunt-exec": "~3.0.0",
    "mocha": "~4.0.1",
    "chai": "~4.1.2",
    "nyc": "~11.3.0",
    "coveralls": "~3.0.0",
    "jsjws": "~5.0.1",
    "ws": "~3.3.2",
    "fastest-writable": "~2.0.0",
    "webpack": "3.8.1",
    "json-loader": "~0.5.7",
    "rimraf": "~2.6.2",
    "eventsource": "~1.0.5",
    "removeNPMAbsolutePaths": "~1.0.2",
    "documentation": "~5.3.3",
    "matic": "~0.5.2"
  },
  "engines": {
    "node": ">= 8"
  }
}
